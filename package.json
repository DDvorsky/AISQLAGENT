{
  "name": "aisqlagent",
  "version": "1.0.0",
  "description": "Remote probe/agent for AISQLWatch - executes SQL commands and provides file access",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && npm run build:ui",
    "build:ui": "cd ui && npm run build",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src/"
  },
  "keywords": [
    "sql",
    "agent",
    "probe",
    "aisqlwatch"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mssql": "^10.0.2",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mssql": "^9.1.5",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.55.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
